{
    "name": "alterphp/easyadmin-mongo-odm-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides support for Doctrine Mongo ODM documents in EasyAdmin",
    "keywords": ["admin", "easyadmin", "odm", "mongo", "document"],
    "homepage": "https://github.com/alterphp/EasyAdminMongoOdmBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Charles Bertineau",
            "email": "pc.bertineau@alterphp.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "easycorp/easyadmin-bundle": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "~5",
        "psr/log": "~1.0",
        "ramsey/uuid": "~3.6",
        "symfony/browser-kit": "~3.0|^4.0",
        "symfony/console": "~3.0|^4.0",
        "symfony/css-selector": "~3.0|^4.0",
        "symfony/dom-crawler": "~3.0|^4.0",
        "symfony/phpunit-bridge": "~3.0|^4.0",
        "symfony/polyfill": "~1.0",
        "symfony/templating": "~3.0|^4.0",
        "symfony/thanks": "^1.0",
        "symfony/twig-bundle": "~3.0|^4.0",
        "symfony/var-dumper": "~3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "AlterPHP\\EasyAdminMongoOdmBundle\\": "src/" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTestBundle\\": "tests/Fixtures/AppTestBundle/",
            "AlterPHP\\EasyAdminMongoOdmBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
